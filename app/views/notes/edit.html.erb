<h1>Edit Note</h1>
<%= form_for @note do |f| %>
   <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :notes %>
  <%= f.text_area :notes %>

  <%= f.label :delivery_date %>
  <%= f.text_field :delivery_date  %>

  <%= f.submit %>
<% end %>